<TextureAtlas imagePath="Sprites.png" width="32" height="28">
    @mixin sprite-dropdown { background: $url; background-position: -0px -0px; width: 10px; height: 10px; }
    @mixin sprite-dropup { background: $url; background-position: -10px -0px; width: 10px; height: 10px; }
    @mixin sprite-pen { background: $url; background-position: -0px -10px; width: 17px; height: 18px; }
